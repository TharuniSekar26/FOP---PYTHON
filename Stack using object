#simple stack using object

class Stack:
  def __init__(self):
    self.items = []
  def push(self,item):
    self.items.append(item)
  def pop(self):
    return self.items.pop()
  def isEmpty():
    return len(self.items)==0
  def peek():
    return self.items[-1]
  def size(self):
    return len(self.items)
#object

s1= Stack()
s1.push(1)
s1.push(2)
s1.push(3)
print(s1) #[1,2,3]
print(s1.pop())
prnt(s1)
print(s1.size())
